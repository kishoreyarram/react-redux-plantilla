{
  "name": "react-redux-plantilla",
  "version": "1.0.1",
  "description": "Plantilla para iniciar en react",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "webpack-dev-server --info false",
    "lint": "node_modules/.bin/esw --color src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha --compilers js:babel-core/register src/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "coverage": "NODE_ENV=test ./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha  -- --compilers js:babel-core/register src/**/*.test.js"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "betotto",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/betotto/react-redux-plantilla.git",
    "type": "git"
  },
  "dependencies": {
    "axios": "0.15.2",
    "immutable": "3.8.1",
    "lodash": "4.16.6",
    "moment": "2.15.2",
    "react": "15.3.2",
    "react-bootstrap": "0.30.6",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-register": "6.11.5",
    "chai": "3.5.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.13",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "2.5.3",
    "mock-browser": "0.92.12",
    "moment": "2.14.1",
    "nock": "8.0.0",
    "npm-run-all": "2.3.0",
    "redux-mock-store": "1.2.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "chai": "3.5.0",
     "mocha": "2.5.3"
  }
}
